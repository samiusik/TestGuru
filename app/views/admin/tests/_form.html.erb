<%= form_with model: [:admin, @test], local: true do |form|  %>

  <%= render 'shared/errors', resource: @test %>
  
    <div class="form_group"><p>
       <%= form.label :title %> <br>
       <%= form.text_field :title %>
    </p></div>

    <div class="form_group"><p>
       <%= form.label :level  %> <br>
       <%= form.number_field :level %>
    </p></div>

    <div class="form_group"><p>
      <%= form.label :category_id %>:
      <%= form.collection_select :category_id, Category.all, :id, :title,
      promt: true %>
    </p></div>

    <p> <%= form.submit %> </p>
<% end %>

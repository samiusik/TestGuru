<tr>
<th><%= test.title %></th>
    <td><%= test.level %></td>
    <td><%= test.questions.count %></td>
    <td><%= link_to t('actions.show'), admin_test_path(test), class: 'btn btn-primary' %></td>
    <td><%= link_to t('actions.edit'), edit_admin_test_path(test), class: 'btn btn-primary' %></td>
    <td>
      <%= link_to t('actions.delete'), admin_test_path(test), class: 'btn btn-primary',
                  method: :delete,
                  data: { confirm: t('actions.delete_confirmation') } %></td>
</tr> 
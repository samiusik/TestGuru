<%= form_with url: sessions_path, local: true do |form| %>
  <div class="form-group">
    <%= form.label :email %>
    <%= form.email_field :email, id: :email %>
  </div>

  <div class="form-group">
    <%= form.label :password %>
    <%= form.password_field :password, id: :password %>
  </div>

  <div class="submit-wrapper">
    <%= form.submit 'Enter' %>
  </div>
<% end %>